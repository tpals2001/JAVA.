package Practice;
import java.util.Scanner;
public class Excercise6_2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		sc.nextLine();
		String S = sc.nextLine();
		int sum =0; 
		
		
		for(int i=0; i<S.length(); i++) {
			
			sum += (S.charAt(i) - '0');
			
			
		}System.out.println(sum);
		
		
		
	}
		


  	}
