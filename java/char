package Practice;
import java. util.Scanner;
public class Array_04 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		String str = "12345";
		
		int sum = 0;
		
		for(int i=0; i<str.length() ; i++) {
			sum += str.charAt(i) - '0' ;
		}System.out.println(sum);
	
	
	
	
	}

}
