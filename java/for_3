
package Practice;
import java. util.Scanner;
public class Array_04 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		
		for(int x=0; x<=10; x++) {
			for(int y=0; y<=10; y++) {
				if(2*x + 4*y == 10) {
					System.out.println("x:"+x+","+"y:"+y);  
				}
			}
	
	
	
	
	}
	
	
	
	
	}

}
