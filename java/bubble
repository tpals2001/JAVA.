package Practice;

public class StudentCard {

	public static void main(String[] args) {
		
		int[] arr = {3,5,6,7,1,2,9,10};
		int tmp = 0;
		for(int i=0; i < arr.length ; i++) {
			for(int j=0; j<arr.length-1-i ; j++) {
				if(arr[j]> arr[j+1]) {
					tmp = arr[j+1];
					arr[j+1] = arr[j];
					arr[j] = tmp ; 	
				}
				
				
				
			}
		}System.out.println(java.util.Arrays.toString(arr));
	}
	
	
}
